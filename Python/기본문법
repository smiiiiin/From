URL= "http://youtube.com"
st= URL.replace("http://", "")
print(st)
st= st[:st.index(".")]
print(st)
pas= st[:3]+str(len(st))+str(st.count('e'))+"!"
print(pas)
print(f"{URL}의 비밀번호는 {pas}임")

